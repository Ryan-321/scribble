<h2>Welcome to Scribbler</h2>

<% @posts.each do |post| %>
<ul>
  <li>
    <p><%= post.title %></p>
  </li>
  <li>
    <%= link_to post.author , post_path(post) %>
  </li>
  <li>
    <p><%= post.article %></p>
  </li>
  <li>
    <p>Date Created <%= post.created_at %></p>
  </li>
</ul>
  <% end %>

  <%= link_to "Create New Post" , new_post_path(@post) %>
